<Window x:Class="VoxelEngenLauncher.Layouts.WindowTab.AddCustumVersion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoxelEngenLauncher.Layouts.WindowTab"
        mc:Ignorable="d"
        Title="AddCustumVersion" Height="300" Width="600" MinHeight="300" MinWidth="600" WindowStartupLocation="CenterOwner" Background="#7EFFFFFF">
    <Grid>
        <Grid Margin="0,10,10,10" Background="#7FFFFFFF" HorizontalAlignment="Right" Width="226">
            <TextBox x:Name="eEB_DirectoryName" HorizontalAlignment="Center" Margin="0,26,0,0" TextWrapping="Wrap" Text="custom_version" VerticalAlignment="Top" Width="206"/>
            <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Имя кастомной версии" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="10,44,0,0" TextWrapping="Wrap" Text="Версия ядра" VerticalAlignment="Top"/>
            <ComboBox x:Name="eCB_CoreVersionList" HorizontalAlignment="Center" Margin="0,60,0,0" VerticalAlignment="Top" Width="206" Height="21"/>
            <Button x:Name="eB_CreatePork" Content="Создать" Margin="0,0,10,35" HorizontalAlignment="Right" Width="47" Height="20" VerticalAlignment="Bottom" Click="eB_CreatePork_Click"/>
            <Button x:Name="eB_Cancel" Content="Отмена" Margin="0,0,62,35" HorizontalAlignment="Right" Width="47" Height="20" VerticalAlignment="Bottom" Click="eB_Cancel_Click"/>
            <ProgressBar x:Name="nePB_Compilate" Margin="10,234,10,0" Height="20" VerticalAlignment="Top"/>
            <Grid Margin="10,86,10,60" Background="#806F6868">
                <Label Content="Информация о модификации"/>
                <TextBlock Text="{Binding Description}"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
