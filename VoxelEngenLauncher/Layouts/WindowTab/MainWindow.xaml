<Window x:Class="VoxelEngenLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoxelEngenLauncher"
        mc:Ignorable="d"
        xmlns:svg="http://sharpvectors.codeplex.com/svg/"
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        Title="VLauncher" Height="558" Width="958" MinHeight="555" MinWidth="955">
    <Grid>
        <Grid Margin="10,10,0,10" Background="#CA717171" OpacityMask="White" HorizontalAlignment="Left" Width="260" >
            <Button x:Name="eB_Play" Content="Играть" HorizontalAlignment="Center" Margin="0,125,0,0" VerticalAlignment="Top" Width="240" Height="42"/>
            <Button x:Name ="eB_Redact" Content="Настройка ресурсов игры" HorizontalAlignment="Center" Margin="0,172,0,0" VerticalAlignment="Top" Width="240" Height="42"/>
            <Button x:Name="eB_FolderGame" Content="Папка игры" HorizontalAlignment="Center" Margin="0,219,0,0" VerticalAlignment="Top" Width="240" Height="42"/>
            <Image Source="{svgc:SvgImage Source=/Resurces/Logo.svg}" x:Name="neIB_LogoGame" HorizontalAlignment="Left" Height="70" Margin="10,10,0,0" VerticalAlignment="Top" Width="240"/>
            <ComboBox HorizontalAlignment="Center" Margin="0,98,0,0" VerticalAlignment="Top" Width="240"/>
            <Button x:Name="eB_GeimerProfile" Content="Профиль игрока" HorizontalAlignment="Center" VerticalAlignment="Top" Width="240" Height="42" Margin="0,266,0,0"/>
        </Grid>
        <Grid Margin="0,10,24,10" HorizontalAlignment="Right" Width="637"/>
    </Grid>
    <Window.Resources>
        
    </Window.Resources>
</Window>
    
